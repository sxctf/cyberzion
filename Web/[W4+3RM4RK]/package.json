{
    "name": "watermark",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev:front": "webpack serve",
        "dev:server": "NODE_ENV=development ts-node server/index.ts",
        "build:front": "webpack --mode=production",
        "build:server": "tsc server/index.ts --outDir task --esModuleInterop",
        "build": "yarn build:front && yarn build:server",
        "run:server": "yarn build:server && node task/index.js",
        "start": "yarn build && npx http-server ./build -a localhost -p 3003 -o",
        "lint": "eslint ."
    },
    "dependencies": {
        "axios": "1.7.2",
        "classnames": "2.5.1",
        "express": "4.19.2",
        "mmmagic": "https://github.com/picturae/mmmagic.git#node-22",
        "multer": "1.4.5-lts.1",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@babel/cli": "7.24.8",
        "@babel/core": "7.24.9",
        "@babel/plugin-transform-runtime": "7.24.7",
        "@babel/preset-env": "7.25.0",
        "@babel/preset-react": "7.24.7",
        "@babel/preset-typescript": "7.24.7",
        "@eslint/js": "9.8.0",
        "@types/express": "4.17.21",
        "@types/multer": "1.4.11",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/webpack-env": "1.18.5",
        "autoprefixer": "10.4.19",
        "babel-loader": "9.1.3",
        "babel-plugin-module-resolver": "5.0.2",
        "copy-webpack-plugin": "12.0.2",
        "css-loader": "7.1.2",
        "cssnano": "7.0.4",
        "eslint": "9.8.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-react": "7.35.0",
        "globals": "15.8.0",
        "html-webpack-plugin": "5.6.0",
        "loader-utils": "3.3.1",
        "mini-css-extract-plugin": "2.9.0",
        "postcss": "8.4.40",
        "postcss-loader": "8.1.1",
        "postcss-nesting": "12.1.5",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "9.6.0",
        "prettier": "3.3.3",
