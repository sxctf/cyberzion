{
    "name": "secure-chat",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev:front": "webpack serve",
        "dev:server": "NODE_ENV=development babel-node server/index.js --dev",
        "build:front": "webpack --mode=production",
        "build:server": "babel server --out-dir task",
        "build:assets": "mkdir -p task/public && cp public/light.css public/dark.css task/public",
        "build": "yarn build:front && yarn build:server && yarn build:assets",
        "run:app": "node task/index.js",
        "start": "yarn build && npx http-server ./build -a localhost -p 3003 -o",
        "lint": "eslint ."
    },
    "dependencies": {
        "axios": "1.7.7",
        "body-parser": "1.20.3",
        "classnames": "2.5.1",
        "cookie-parser": "1.4.7",
        "cors": "2.8.5",
        "express": "4.21.1",
        "express-ws": "5.0.2",
        "puppeteer": "23.6.0",
        "qs": "6.13.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "uuid": "10.0.0"
    },
    "devDependencies": {
        "@babel/cli": "7.25.7",
