{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "CommonJS",
    "outDir": "./build/",
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }],
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "target": "ES6"
  },
  "exclude": ["node_modules"]
}
